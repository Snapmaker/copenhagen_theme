

<script>
  const sectionsIDs = [
    {{#each category.sections}}
      {{#is description 'Manual-Type' }}
        '{{id}}',
      {{/is}} 
    {{/each}}
  ]
  const locale = '{{help_center.locale}}'
  window.addEventListener('DOMContentLoaded', function () {
      setTimeout(()=>handleSectionResource('{{category.id}}', '{{help_center.locale}}'))
  })
</script>

