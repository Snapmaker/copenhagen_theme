            <div class="mt-l mb-xs compatible-wrapper" id="compatible-label-container">
              
            </div>
            <div class="products-label-btn-container pb-xl" id="compatible-container">
              {{! insert }}
            </div>
          </div>
          <div class="download-select ml-xl" id="file-resource-container">
            {{! insert }}
            <div id="placeholder-software"></div>
            <div id="placeholder-firmware"></div>
            <div id="placeholder-cura-plugins"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
